<h1>Create Video Call</h1>
<p>Start a new video call.</p>
<%= form_with model: @video_call do |form| %>
  <%= form.label :name, "Room Name" %>
  <%= form.text_field :name, required: true %>
  <%= form.submit 'Create Video Call' %>
<% end %>
<% flash.each do |type, message| %>
  <mark class="<%= type %>">
    <%= message %>
  </mark>
<% end %>
</body>
